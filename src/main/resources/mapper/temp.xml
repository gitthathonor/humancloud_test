<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.metacoding.humancloud.domain.temp.TempDao">
	<insert id="save" useGeneratedKeys="true" keyProperty="companyId"> 
		INSERT INTO temp(username, password, logo)
		VALUES(#{username}, #{password}, #{logo})
	</insert>
	<select id="findById" resultType="site.metacoding.humancloud.domain.temp.Temp"></select>
	<select id="findAll" resultType="site.metacoding.humancloud.domain.temp.Temp"></select>
	<update id="update"></update>
	<delete id="deleteById"></delete>
</mapper>